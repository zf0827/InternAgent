# Base Model API Key for InternAgent
# Copy this file to LLM.env and fill in your API keys

# OpenAI-compatible API (required)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Anthropic API (optional)
ANTHROPIC_API_KEY=

# DeepSeek API (optional, alternative to OpenAI)
DS_API_KEY=your_deepseek_api_key_here
DS_API_BASE_URL=https://api.deepseek.com/v1

# Search engine API keys
GOOGLE_API_KEY=your_google_api_key_here
SERPER_KEY_ID=your_serper_key_id_here
SERP_API_KEY=your_serp_api_key_here

# GitHub API token (for code search)
GITHUB_AI_TOKEN=your_github_token_here

# Kaggle API configuration
# Set this to the path of your kaggle.json file, or leave empty to use ~/.kaggle/kaggle.json
KAGGLE_CONFIG_DIR=

# Jina API key (for web search)
JINA_API_KEY=your_jina_api_key_here

